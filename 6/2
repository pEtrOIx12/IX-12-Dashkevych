#define 2

#include <stdio.h>

int main() {
printf("/%d/\n",557);
printf("/%10d/\n",557);
printf("/%-10d/\n",557);

printf("/%8d/\n",21949);
printf("/%8d/\n",-1611);
printf("/%8d/\n",-8380);
printf("/%8d/\n",2143);

printf("/%f/\n", 6543.21);
printf("/%e/\n", 6543.21);
printf("/%4.2f/\n", 6543.21);
printf("/%3.1f/\n", 6543.21);
printf("/%10.3f/\n", 6543.21);
printf("/%10.3e/\n", 6543.21);
}
