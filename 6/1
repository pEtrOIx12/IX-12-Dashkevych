#define PI 3.14159

#include <stdio.h>

int main() {
  int a = 5;
  float b = 23.5;
  int c = 31000;

  printf("%d метрів тканини коштувало %f гривень. \n", a, b);
  printf("Значення числа п рівне %.5f.\n", PI);

  printf("IBM сумісні комп'ютери набули широкого розповсюдження. \n");
  printf("%c%d\n", '$', c);

  return 0;
}
